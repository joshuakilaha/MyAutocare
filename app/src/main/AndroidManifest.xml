<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myautocare">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Admin.AdminViews.BMW.ImagesActivity"></activity>
        <activity android:name=".Admin.AdminViews.BMW.BMWEdit" />
        <activity android:name=".Admin.AdminHome" />
        <activity android:name=".Admin.AdminSignup" />
        <activity android:name=".Admin.AdminLogin" />
        <activity android:name=".Activities.LoginActivity" />
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>